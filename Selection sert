data = [5,3,1,2]
for i in range(len(data)-1):
    print("Proses ke",(i+1))
    for x in range (i+1,len(data)):
        if data[i]>data[x]:
            temp = data[i]
            data[i] = data[x]
            data[x] = temp                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
    print(data)
